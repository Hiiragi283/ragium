{
  "multipart": [
    {
      "apply": {
        "model": "ragium:block/fluid_pipe_station"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay",
        "x": 90
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay",
        "x": 270
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay"
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay",
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay",
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "ragium:block/pipe_overlay",
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    }
  ]
}