{
  "schemaVersion": 1,
  "id": "ragium",
  "version": "${version}",
  "name": "Ragium",
  "description": "A simple tech mod for Fabric",
  "authors": [
    "Hiiragi283"
  ],
  "contact": {
    "sources": "https://github.com/Hiiragi283/ragium",
    "issues": "https://github.com/Hiiragi283/ragium/issue"
  },
  "license": "MPL-2.0",
  "icon": "assets/ragium/textures/item/ragium.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "hiiragi283.ragium.common.RagiumCommon",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "hiiragi283.ragium.client.RagiumClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "hiiragi283.ragium.data.RagiumDataGenerator",
        "adapter": "kotlin"
      }
    ],
    "ragium.plugin.server": [
      {
        "value": "hiiragi283.ragium.common.internal.RagiumDefaultPlugin",
        "adapter": "kotlin"
      },
      {
        "value": "hiiragi283.ragium.common.internal.RagiumHardModePlugin",
        "adapter": "kotlin"
      }
    ],
    "ragium.plugin.client": []
  },
  "accessWidener": "ragium.accesswidener",
  "mixins": [
    "ragium.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16",
    "minecraft": ">=1.21- <1.22-",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  }
}
